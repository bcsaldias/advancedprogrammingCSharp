<UserControl x:Class="Tarea4.Separador"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="70" d:DesignWidth="70">
    <Canvas Height="70" Width="70">
        <Ellipse Fill="#FFDF35E7" Height="70" Width="70" Canvas.Left="0" Canvas.Top="0" >  </Ellipse>
        <Ellipse Fill="Gainsboro" Height="26" Width="27" Canvas.Left="43" Canvas.Top="21">  </Ellipse>
        <TextBlock Canvas.Left="0" Canvas.Top="0" Height="70" Visibility="Hidden" Name="textBlock1" Width="70" />
        <Ellipse Canvas.Left="0" Canvas.Top="21" Fill="Gainsboro" Height="26" Width="27" PreviewMouseRightButtonDown="Ellipse_PreviewMouseRightButtonDown" />
        <Label Canvas.Left="0" Canvas.Top="21" Content="In" Height="26" Name="label1" Width="34" PreviewMouseRightButtonDown="label1_PreviewMouseRightButtonDown" />
        <Label Canvas.Left="40" Canvas.Top="21" Content="Out" Height="26" Name="label2" Width="39" PreviewMouseRightButtonDown="label2_PreviewMouseRightButtonDown" />
    </Canvas>
</UserControl>
